{
  "concatenate": [
    ".gitignore",
    "README.md",
    ".npmrc"
  ],
  "copy": {
    "boilerplate": "/"
  },
  "generate": {
    "vue.config.template.js": "vue.config.js",
    "src/.pristine/imports.template.js": "src/.pristine/imports.js",
    "config/storybook/main.template.js": "config/storybook/main.js"
  },
  "move": {
    "src": {
      "vue-cli-symfony-4": "assets/app/"
    }
  },
  "remove": {
    "vue.config.template.js": "*",
    "src/.pristine/imports.template.js": "*",
    "config/storybook/main.template.js": "*",
    "public/index.html": [
      "vue-cli-symfony-4"
    ],
    "public/favicon.ico": [
      "vue-cli-symfony-4"
    ]
  },
  "scripts": {
    "update:packages": "npm update --save/--save-dev",
    "serve": {
      "vue-cli-symfony-4": "vue-cli-service build-watch --mode development"
    }
  }
}
